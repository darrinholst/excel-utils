{
  "name": "excel-utils",
  "version": "1.0.0",
  "description": "Some utilities if you're programmatically working with spreadsheets",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "pretest": "npm run compile",
    "test": "mocha -c **/*.spec.js",
    "compile": "rimraf dist && tsc",
    "prepublishOnly": "npm run verify",
    "verify": "npm run lint && npm run test"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "bracketSpacing": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darrinholst/excel-utils.git"
  },
  "keywords": [
    "excel",
    "spreadsheet"
  ],
  "author": "Darrin Holst <darrinholst@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/darrinholst/excel-utils",
  "bugs": {
    "url": "https://github.com/darrinholst/excel-utils/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "release-it": "^12.3.6",
    "rimraf": "^3.0.0",
    "typescript": "^3.5.3"
  }
}
